        <Button Content="Editar" HorizontalAlignment="Left" Margin="234,219,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_4"/>
        <Label Content="Nombre" HorizontalAlignment="Left" Margin="0,85,0,0" VerticalAlignment="Top" Width="65"/>
        <TextBox x:Name="Nom" HorizontalAlignment="Left" Height="23" Margin="84,85,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <DataGrid x:Name="DataCiu" HorizontalAlignment="Left" Margin="252,10,0,0" VerticalAlignment="Top" Height="132" Width="247" SelectionChanged="DataGrid_SelectionChanged_1"/>
